<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ProductEntity" table="product" schema="spring_docker">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="missionName">
            <column name="mission_name" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="acquisitionDate">
            <column name="acquisition_date" sql-type="datetime" not-null="true"/>
        </property>
        <property name="productFootprint">
            <column name="product_footprint" sql-type="json" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(64)" length="64"/>
        </property>
    </class>
</hibernate-mapping>